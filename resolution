#include <stdio.h>
#include <stdlib.h>
#include <math.h>
int main(){
	
	int n1, n2, a, b, prod = 0;
	printf("Informe dois numeros inteiros: ");
	scanf("%d%d", &n1, &n2);
	a = n1;
	b = n2;
	while (a !=0){
	    if((a % 2) == 1);
	    prod += b;
	    a /= 2;
	    b*=2;
	}
	printf("O produto de %d e %d eh %d", n1, n2, prod);
}
